# Azure AI Search + Azure OpenAI Sample Environment
# Rename this file to .env and fill in the values before running scripts

# Azure AI Search Configuration
AZURE_SEARCH_SERVICE_ENDPOINT=https://<your-search-service>.search.windows.net
AZURE_SEARCH_ADMIN_KEY=<your-search-admin-key>
AZURE_SEARCH_INDEX_NAME=vector-document-index
AZURE_SEARCH_SEMANTIC_CONFIG=default

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://<your-openai-service>.openai.azure.com
AZURE_OPENAI_KEY=<your-openai-key>
AZURE_OPENAI_EMBEDDING_NAME=text-embedding-ada-002
AZURE_OPENAI_API_VERSION=2023-12-01-preview

# Web App Configuration (optional)
FLASK_APP=sample_web_app.py
FLASK_ENV=development
PORT=5000
